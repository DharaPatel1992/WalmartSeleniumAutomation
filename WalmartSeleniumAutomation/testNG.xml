<?xml version="1.0" encoding="UTF-8"?>

<suite name="Walmart Automation with Selenium">
	<listeners>
		<listener class-name="com.walmart.qa.ExtentReportListener.ExtentReporterNG" />
	</listeners>

	<test name="Walmart automation testNG" allow-return-values="true">
		<classes>
			<class name="com.walmart.qa.testcases.SignUpPageTest" />
			<class name="com.walmart.qa.testcases.LoginPageTest" />
			<class name="com.walmart.qa.testcases.HomePageTest" />
		</classes>
	</test>
</suite>
